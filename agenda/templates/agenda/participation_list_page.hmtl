{% load static %}

{% load i18n %}
{% load endless %}

{% for participant in participation_list %}
    <div class="col-md-4">
        <img class="img-responsive thumbnail" alt="avatar" src="
            {% if participation.user.profile.avatar.small %}
                {{ participation.user.profile.avatar.small.url }}
            {% else %}
                {% static 'ressources/logo/icon_bronycub.png' %}
            {% endif %}
        "/>
    </div>
{% endfor %}
{% show_more 'more comments' %}
